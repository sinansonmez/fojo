{
  "name": "fojo",
  "version": "0.1.0",
  "description": "Go-powered PDF generation for Node and the browser",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build:go": "cd go && make all",
    "build:ts": "tsgo",
    "build": "npm run build:go && npm run build:ts",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "engines": {
    "node": ">=18"
  },
  "author": "Sinan Chaush (Sonmez)",
  "license": "MIT",
  "dependencies": {
    "@typescript/native-preview": "^7.0.0-dev.20251230.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.3"
  }
}
