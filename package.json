{
  "name": "fojo",
  "version": "0.2.0",
  "description": "Go-powered PDF generation for Node and the browser",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "url": "https://github.com/sinansonmez/fojo"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest --config jest.config.json",
    "lint": "tslint -p tsconfig.json",
    "build:go": "cd go && make all",
    "build:ts": "tsgo",
    "build": "npm run build:go && npm run build:ts",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "Sinan Chaush (Sonmez)",
  "license": "MIT",
  "dependencies": {
    "@typescript/native-preview": "^7.0.0-dev.20251230.1"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "jest": "^29.4.1",
    "ts-jest": "^29.4.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0"
  }
}
